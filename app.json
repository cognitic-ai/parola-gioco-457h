{
  "expo": {
    "name": "Giochi Italiani",
    "slug": "giochi-italiani",
    "orientation": "default",
    "scheme": "giochi-italiani",
    "userInterfaceStyle": "automatic",
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "experiments": {
      "reactCompiler": true
    },
    "plugins": [
      "expo-font",
      "expo-image",
      "expo-router",
      "expo-web-browser",
      "@bacons/apple-colors"
    ]
  }
}
